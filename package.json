{
  "name": "meta",
  "description": "information about interacting with the group and a place to ask general questions",
  "license": "MIT",
  "author": "dsmJS",
  "repository": "dsmjs/meta",
  "bugs": "https://github.com/dsmjs/meta/issues",
  "homepage": "https://github.com/dsmjs/meta#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "test": "run-s lint:*",
