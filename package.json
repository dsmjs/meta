  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.2.0",
    "globstar": "1.0.0",
    "husky": "4.2.5",
    "markdownlint-cli": "0.23.2",
    "npm-run-all": "4.1.5",
    "validate-commit-msg": "2.14.0"
  }
}
